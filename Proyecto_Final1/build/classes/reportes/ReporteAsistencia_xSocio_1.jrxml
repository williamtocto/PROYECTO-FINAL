	<queryString>
		<![CDATA[select  cedula_socio,nombre_socio,apellido_socio,fecha_reunion,a.codigo_asistencia,a.estado_asistencia
   from socio s,reunion r, asistencia a where
       s.codigo_socio=a.codigo_socio_asis and r.codigo_reunion= a.codigo_reunion and estado_asistencia= $P{estado} and (fecha_reunion)  between $P{fecha_inicio} and $P{fecha_fin} AND cedula_socio=$P{cedula} order by fecha_reunion;]]>
