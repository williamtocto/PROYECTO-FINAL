	<queryString>
		<![CDATA[SELECT s.codigo_socio,nombre_socio,apellido_socio,tipo_transaccion,numero_de_cuenta,fecha_trans,monto,saldo 
                    FROM socio s, transaccion t where (fecha_trans) 
                     between $P{fecha_inicio} and $P{fecha_fin} and s.codigo_socio=t.codigo_socio and 
		numero_de_cuenta<>1001
                     order by fecha_trans;]]>
	</queryString>
